<Window x:Class="Bash_Autoservis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bash_Autoservis"
        mc:Ignorable="d"
        Title="Башкирцев" FontFamily="Tw Cen MT" Height="450" Width="800" Icon="/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/service_logo.ico" HorizontalAlignment="Left" />
        <TextBlock Text="Услуги Автосервиса" HorizontalAlignment="Center" FontSize="30" VerticalAlignment="Center"></TextBlock>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Content="Назад" FontFamily="Tw Cen MT" Name="BtnBack" HorizontalAlignment="Right" Margin="5"
                Click="BtnBack_Click"></Button>
    </Grid>
</Window>
